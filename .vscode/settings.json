{
    "todo-tree.general.tags": [
        "TODO",
        "FIXME",
        "BUG",
        "HACK",
        "NOTE",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.highlights.defaultHighlight": {
        "icon": "check",
        "type": "text",
        "foreground": "#ffffff",
        "opacity": 50
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check-circle",
            "foreground": "#FF00B6",
            "iconColour": "#FF00B6"
        },
        "FIXME": {
            "icon": "alert",
            "foreground": "#FF0045",
            "iconColour": "#FF0045"
        },
        "BUG": {
            "icon": "bug",
            "foreground": "#FF0045",
            "iconColour": "#FF0045"
        },
        "NOTE": {
            "icon": "note",
            "foreground": "#4A90E2",
            "iconColour": "#4A90E2"
        }
    },
    "todohighlight.keywords": [
        {
            "text": "TODO:",
            "color": "#FF00B6",
            "backgroundColor": "transparent",
            "overviewRulerColor": "#FF00B6"
        },
        {
            "text": "FIXME:",
            "color": "#FF0045",
            "backgroundColor": "transparent",
            "overviewRulerColor": "#FF0045"
        },
        {
            "text": "NOTE:",
            "color": "#4A90E2",
            "backgroundColor": "transparent",
            "overviewRulerColor": "#4A90E2"
        }
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/vendor": true,
        "**/var/cache": true,
        "**/var/log": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/vendor": true,
        "**/var": true,
        "**/*.min.js": true
    },
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "[twig]": {
        "editor.tabSize": 4
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "intelephense.stubs": [
        "apache",
        "bcmath",
        "bz2",
        "calendar",
        "Core",
        "ctype",
        "curl",
        "date",
        "dom",
        "fileinfo",
        "filter",
        "ftp",
        "gd",
        "hash",
        "iconv",
        "json",
        "libxml",
        "mbstring",
        "mysqli",
        "mysqlnd",
        "openssl",
        "pcre",
        "PDO",
        "pdo_mysql",
        "Phar",
        "SimpleXML",
        "sodium",
        "SPL",
        "standard",
        "tokenizer",
        "xml",
        "xmlreader",
        "xmlwriter",
        "zip",
        "zlib"
    ]
}